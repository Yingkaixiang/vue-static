{
  "name": "vue-static",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:dev": "rimraf dist && cross-env NODE_ENV=development yarn run build:client && cross-env NODE_ENV=development yarn run build:server",
    "build:client": "webpack --config build/webpack.client.config.js",
    "build:server": "webpack --config build/webpack.server.config.js",
    "commit": "yarn git-cz",
    "test": "jest",
    "server": "cross-env NODE_ENV=production node server/index.js",
    "dev": "cross-env NODE_ENV=development node server/index.js",
    "analyzer": "cross-env NODE_ENV=production webpack --config build/webpack.analyzer.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "img-loader": "^3.0.1",
    "jest": "^24.8.0",
    "koa-webpack-middleware": "^1.0.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-aspect-ratio-mini": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-px-to-viewport": "^1.1.0",
    "postcss-write-svg": "^3.0.1",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.0.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-spritesmith": "^1.0.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "koa": "^2.7.0",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lru-cache": "^5.1.1",
    "mockjs": "^1.0.1-beta3",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
